--Question1

SELECT city,
country
FROM city ci
INNER JOIN country co ON ci.country_id =co.country_id; 

--Question2

SELECT payment_id,
first_name,
last_name
FROM payment
INNER JOIN customer USING (customer_id);

--Question3

SELECT rental_id,
first_name,
last_name
FROM rental r
INNER JOIN customer c ON r.customer_id=c.customer_id; 


